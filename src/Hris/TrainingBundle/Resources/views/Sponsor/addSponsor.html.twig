{% extends '::base.html.twig' %}

{% block right_content -%}
    <div class="span12">
        <div class="span7">
            <h3>
                <i class="icon-circle-info icon-largest" title="Show"></i>Adding Sponsor Lists
            </h3>
          </div>
    </div>

    <form action="{{ path('createSponsor') }}" method="post" {{ form_enctype(form) }}>
        {{ form_widget(form) }}
        <br/>
        <ul class="trainig_actions inline" style="float:left;">
            <li>
                <button type="submit" class="btn">Register</button>
            </li>
            <li>
                <a href="{{ path('sponsors') }}" class="btn">
                    Back to the list
                </a>
            </li>
        </ul>
    </form>

{% endblock %}
